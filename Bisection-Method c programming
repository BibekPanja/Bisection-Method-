#include<stdio.h>
#include<math.h>

#define f(x) (x*x*x-9*x+1)

int main(){
	float a,b,c;
	while(1){
		printf("Enter the interval:\n");
		scanf("%f %f",&a,&b);
		if(f(a)*f(b)<0){
			break;
		}
		else{
			
			printf("wrong interval\n");		
			}
	}
	do{
		

	c=(a+b)/2;
	if(f(a)*f(c)<0){
		b=c;
	}
	else
	{
		a=c;
	}}
	while(fabs f(c)>0.0001);
	printf("The root is=%f",c);
	return 0;
	
}
